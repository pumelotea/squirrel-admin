{
  "name": "lt-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode dev --dest=dist-dev",
    "build": "vue-cli-service build --mode prod --dest=dist-prod",
    "lint": "vue-cli-service lint",
    "build-docker": "vue-cli-service build --mode prod --dest=dist-prod && docker build . -t lt-admin && docker save > lt-admin.img lt-admin:latest",
    "build-prod": "vue-cli-service build --mode prod --dest=dist-prod",
    "build-test": "vue-cli-service build --mode test --dest=dist-test"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "cropperjs": "^1.5.1",
    "vconsole": "^3.3.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vue-scroller": "^2.2.4",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.6.0",
    "@vue/cli-plugin-babel": "^4.2.0",
    "@vue/cli-plugin-eslint": "^4.2.0",
    "@vue/cli-plugin-router": "^4.2.0",
    "@vue/cli-plugin-vuex": "^4.2.0",
    "@vue/cli-service": "^4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "filemanager-webpack-plugin": "^2.0.5",
    "prettier": "^1.19.1",
    "squirrelzoo-build-plugin": "^1.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-pumelo-tea": "^2.3.2",
    "vue-template-compiler": "^2.6.11"
  }
}
